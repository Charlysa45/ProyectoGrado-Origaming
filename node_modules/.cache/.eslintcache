" value={mensaje} onChange={e => setMensaje(e.target.value)} style={{width: '36rem'}} />\n                                    <button className=\"btn btn-primary\">Enviar</button>\n                                </div>\n                            </form>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default MatchPage\n","/home/matr3x/proyecto/src/components/context/ApiContext.js",[],"/home/matr3x/proyecto/src/pages/Error404.js",[],"/home/matr3x/proyecto/src/components/TeamForm.js",[],"/home/matr3x/proyecto/src/services/TeamService.js",[],"/home/matr3x/proyecto/src/pages/TeamPage.js",[],"/home/matr3x/proyecto/src/components/hooks/useTeams.js",[],{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","severity":1,"message":"220","line":66,"column":1,"nodeType":"221","endLine":130,"endColumn":2},{"ruleId":"222","severity":1,"message":"223","line":12,"column":21,"nodeType":"224","messageId":"225","endLine":12,"endColumn":84,"fix":"226"},{"ruleId":"227","severity":1,"message":"228","line":13,"column":21,"nodeType":"224","endLine":13,"endColumn":33},{"ruleId":"227","severity":1,"message":"228","line":14,"column":21,"nodeType":"224","endLine":14,"endColumn":33},{"ruleId":"227","severity":1,"message":"228","line":17,"column":50,"nodeType":"224","endLine":17,"endColumn":62},{"ruleId":"227","severity":1,"message":"228","line":19,"column":50,"nodeType":"224","endLine":19,"endColumn":62},{"ruleId":"227","severity":1,"message":"228","line":21,"column":50,"nodeType":"224","endLine":21,"endColumn":62},{"ruleId":"229","severity":1,"message":"230","line":2,"column":17,"nodeType":"231","messageId":"232","endLine":2,"endColumn":26},{"ruleId":"229","severity":1,"message":"233","line":3,"column":8,"nodeType":"231","messageId":"232","endLine":3,"endColumn":17},{"ruleId":"234","severity":1,"message":"235","line":12,"column":43,"nodeType":"236","messageId":"237",